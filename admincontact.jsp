                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="card">
                        <div class="header">
                            <h2>
                                CONTACT TABLE
                            </h2>
                        </div>
                        <div class="body">
                            <div class="table-responsive">
                                <table class="table table-bordered table-striped table-hover js-basic-example dataTable">
                                    <thead>
                                        <tr>
                                            <th>Contact ID</th>
                                            <th>Name</th>
                                            <th>Email Address</th>
                                            <th>Subject</th>
                                            <th>Description</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                    <%
                                        try{
                                        c = ConnectionCreate.initDatabase();
                                        Statement st1 = c.createStatement();
                                        ResultSet rs1;
                                        rs1 = st1.executeQuery("select * from contact where ad_response='No'");   
                                        while(rs1.next())
                                        {
                                    %>
                                    <tr>
                                        <td><%= rs1.getString(1)%></td>                                      
                                        <td><%= rs1.getString(2)%></td>
                                        <td><%= rs1.getString(3)%></td>
                                        <td><%= rs1.getString(4)%></td>
                                        <td><%= rs1.getString(5)%></td>
                                    </tr>
                                    <% 
                                        }} catch(Exception e) {} finally { c.close(); }
                                    %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
